generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String    @id @default(uuid())
  name       String
  age        Int
  username   String
  email      String    @unique
  password   String
  created_at DateTime
  updated_at DateTime? @updatedAt

  @@map("user")
}
